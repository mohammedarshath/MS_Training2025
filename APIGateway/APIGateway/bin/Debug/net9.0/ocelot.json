{
    "Routes": [
        {
            "ServiceName": "CB-COMMON",
            "DownstreamPathTemplate": "/api/users",
            "UpstreamPathTemplate": "/users",
            "UpstreamHttpMethod": [ "GET" ],
            "FileCacheOptions": {
                "TtlSeconds": 15
            },
            "LoadBalancerOptions": {
                "Type": "RoundRobin"
            }
        },
        {
            "ServiceName": "BOEING-COMMON",
            "DownstreamPathTemplate": "/api/v1/Vehicles",
            "UpstreamPathTemplate": "/vehicles",
            "UpstreamHttpMethod": [ "GET" ],
            "FileCacheOptions": {
                "TtlSeconds": 15
            },
            "LoadBalancerOptions": {
                "Type": "RoundRobin"
            }
        }
        //{
        //    "ServiceName": "VSECURE-COMMON",
        //    "DownstreamPathTemplate": "/api/v1/Categories",
        //    "UpstreamPathTemplate": "/categories",
        //    "UpstreamHttpMethod": [ "GET" ],
        //    "FileCacheOptions": {
        //        "TtlSeconds": 15
        //    },
        //    "LoadBalancerOptions": {
        //        "Type": "RoundRobin"
        //    }
        //}
    ],
    "GlobalConfiguration": {
        "RequestIdKey": "OcRequestId",
        "DownstreamScheme": "http",
        "ReRoutesCaseSensitive": false,
        "UseServiceDiscovery": true,
        "ServiceDiscoveryProvider": {
            "Host": "host.docker.internal",
            "Port": 8761,
            "Type": "Eureka"
        }
    }
}