    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: http://0.0.0.0:80
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      EUREKA__CLIENT__SERVICEURL: http://host.docker.internal:8761/eureka/
      EUREKA__CLIENT__SHOULDFETCHREGISTRY: "true"
      EUREKA__CLIENT__SHOULDREGISTERWITHEUREKA: "true"
      EUREKA__INSTANCE__APPNAME: vehicle-api
      EUREKA__INSTANCE__HOSTNAME: host.docker.internal
      EUREKA__INSTANCE__INSTANCEID: :vehicle-api:1502
      EUREKA__INSTANCE__NONSECUREPORT: "5001"
      EUREKA__INSTANCE__PREFERIPADDRESS: "true"
      NUGET_FALLBACK_PACKAGES: /.nuget/fallbackpackages
    image: vehicleapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /.nuget/packages --additionalProbingPath /.nuget/fallbackpackages  "/app/bin/Debug/net9.0/VehicleAPI.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: dotnet --roll-forward Major /VSTools/DistrolessHelper/DistrolessHelper.dll --stop dotnet
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
      - mode: ingress
        target: 80
