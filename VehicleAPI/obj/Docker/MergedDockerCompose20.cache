services:
  sqlservervehicleservice:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Vignesh@95!
    expose:
      - "1433"
    hostname: sqlserverpolicyservice
    image: mcr.microsoft.com/mssql/server
    networks:
      default: null
