name: dockercompose12812598661636736566
services:
  circuitbreaker:
    build:
      context: E:\boeingsep2025ws\CircuitBreaker
      dockerfile: CircuitBreaker/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
      ASPNETCORE_URLS: http://0.0.0.0:80
      EUREKA__CLIENT__SERVICEURL: http://host.docker.internal:8761/eureka/
      EUREKA__CLIENT__SHOULDFETCHREGISTRY: "true"
      EUREKA__CLIENT__SHOULDREGISTERWITHEUREKA: "true"
      EUREKA__INSTANCE__APPNAME: BOEING-COMMON
      EUREKA__INSTANCE__HEALTHCHECKURL: http://localhost:1502/health
      EUREKA__INSTANCE__HOSTNAME: boeing-common
      EUREKA__INSTANCE__INSTANCEID: boeing-common-localhost:80
      EUREKA__INSTANCE__NONSECUREPORT: "80"
      EUREKA__INSTANCE__PREFERIPADDRESS: "false"
      EUREKA__INSTANCE__STATUSPAGEURL: http://localhost:1502/health
    image: circuitbreaker
    networks:
      boeingnetwork:
        aliases:
          - boeing-common
    ports:
      - mode: ingress
        target: 80
        published: "7076"
        protocol: tcp
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\param\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\param\AppData\Roaming/Microsoft/UserSecrets
        target: /root/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\param\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\param\AppData\Roaming/ASP.NET/Https
        target: /root/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
networks:
  boeingnetwork:
    name: boeingnetwork
    external: true