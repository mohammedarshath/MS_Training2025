name: dockercompose12812598661636736566
services:
  circuitbreaker:
    build:
      context: E:\boeingsep2025ws\CircuitBreaker
      dockerfile: CircuitBreaker/Dockerfile
      args:
        BUILD_CONFIGURATION: Debug
        LAUNCHING_FROM_VS: "true"
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: CircuitBreaker
      target: base
    entrypoint:
      - dotnet
      - --roll-forward
      - Major
      - /VSTools/DistrolessHelper/DistrolessHelper.dll
      - --wait
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: http://0.0.0.0:80
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      EUREKA__CLIENT__SERVICEURL: http://host.docker.internal:8761/eureka/
      EUREKA__CLIENT__SHOULDFETCHREGISTRY: "true"
      EUREKA__CLIENT__SHOULDREGISTERWITHEUREKA: "true"
      EUREKA__INSTANCE__APPNAME: BOEING-COMMON
      EUREKA__INSTANCE__HEALTHCHECKURL: http://localhost:1502/health
      EUREKA__INSTANCE__HOSTNAME: boeing-common
      EUREKA__INSTANCE__INSTANCEID: boeing-common-localhost:80
      EUREKA__INSTANCE__NONSECUREPORT: "80"
      EUREKA__INSTANCE__PREFERIPADDRESS: "false"
      EUREKA__INSTANCE__STATUSPAGEURL: http://localhost:1502/health
      NUGET_FALLBACK_PACKAGES: /.nuget/fallbackpackages
    image: circuitbreaker:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /.nuget/packages --additionalProbingPath /.nuget/fallbackpackages  "/app/bin/Debug/net9.0/CircuitBreaker.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: dotnet --roll-forward Major /VSTools/DistrolessHelper/DistrolessHelper.dll --stop dotnet
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      boeingnetwork:
        aliases:
          - boeing-common
    ports:
      - mode: ingress
        target: 80
        published: "7076"
        protocol: tcp
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    tty: true
    volumes:
      - type: bind
        source: C:\Users\param\AppData\Roaming\Microsoft\UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\param\AppData\Roaming\Microsoft\UserSecrets
        target: /root/.microsoft/usersecrets
        read_only: true
        bind:
