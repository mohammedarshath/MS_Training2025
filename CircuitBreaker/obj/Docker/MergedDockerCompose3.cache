      - type: bind
        source: C:\Users\param\AppData\Roaming\ASP.NET\Https
        target: /root/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: E:\boeingsep2025ws\CircuitBreaker\CircuitBreaker
        target: /app
        bind:
          create_host_path: true
      - type: bind
        source: E:\boeingsep2025ws\CircuitBreaker
        target: /src
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\param\vsdbg\vs2017u5
