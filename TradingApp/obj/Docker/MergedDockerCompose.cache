name: dockercompose7931033021457896441
services:
  tradingapp:
    build:
      context: E:\boeingsep2025ws\TradingApp
      dockerfile: TradingApp/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
    image: tradingapp
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
